    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>dotnetcore_jwt_authentication</RootNamespace>
    <UserSecretsId>9bf28426-20c5-45ab-8807-e0ac5310a0fc</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
